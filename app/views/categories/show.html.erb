<div class="container">
  <div class="row">
    <div class="col-md-8 col-md-offset-2">
      <ul class="list-unstyled">
        <% @images.each do |image| %>
          <li>
            <div class="well row">
              <div class="col-sm-4 text-center">
                <%= image_tag(image.image.url, class: 'atrwork') %>
              </div>
            </div>
          </li>
        <% end %>
      </ul>
    </div>
  </div>
</div>